<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.adroid.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textViewassessmenttitle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="73dp"
            tools:layout_editor_absoluteY="112dp" />

        <TextView
            android:id="@+id/textViewassessmenttype"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@id/textViewassessmenttitle"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="73dp"
            tools:layout_editor_absoluteY="112dp" />

        <TextView
            android:id="@+id/textViewassessmentstart"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@id/textViewassessmenttype"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="73dp"
            tools:layout_editor_absoluteY="112dp" />

        <TextView
            android:id="@+id/textViewassessmentend"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@id/textViewassessmentstart"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="73dp"
            tools:layout_editor_absoluteY="112dp" />

    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>